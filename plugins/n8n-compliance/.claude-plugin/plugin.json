{
  "name": "n8n-compliance",
  "description": "A plugin to remove n8n enterprise features and rebuild them to enforce enterprise compliance.",
  "version": "1.0.0",
  "author": {
    "name": "Adam Haile"
  },
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp@latest"]
    }
  }
}
